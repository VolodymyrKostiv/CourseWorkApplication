<UserControl x:Class="CourseWorkApplication.Controls.AdminNavigationBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:CourseWorkApplication.Controls"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DesignHeight="450"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <Grid>
        <Grid.Resources>
            <ResourceDictionary>
                <ResourceDictionary.MergedDictionaries>
                    <ResourceDictionary Source="/Styles/NavbarStyles.xaml" />
                </ResourceDictionary.MergedDictionaries>
                <Style BasedOn="{StaticResource NavbarRadioButton}" TargetType="RadioButton" />
            </ResourceDictionary>
        </Grid.Resources>
    </Grid>
</UserControl>
